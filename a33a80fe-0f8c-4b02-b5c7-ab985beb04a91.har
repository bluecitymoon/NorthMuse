ndefined?e[h]:h}if(n){for(;m<=n;m++){p=r[m]===\"\"?o.length:r[m];o=o[p]=m<n?o[p]||(r[m+1]&&isNaN(r[m+1])?{}:[]):h}}else{if(b.isArray(f[p])){f[p].push(h)}else{if(f[p]!==undefined){f[p]=[f[p],h]}else{f[p]=h}}}}else{if(p){f[p]=d?undefined:\"\"}}});return f}})(jQuery,this);","comment":""},"redirectURL":"","headersSize":607,"bodySize":705,"comment":""},"cache":{},"timings":{"comment":"","ssl":-1,"blocked":-1,"dns":-1,"send":0,"connect":-1,"wait":8,"receive":0},"serverIPAddress":"180.163.159.101","comment":"","time":9},{"pageref":"https://movie.douban.com/","startedDateTime":"2017-02-27T08:46:22.649Z","request":{"method":"GET","url":"https://img3.doubanio.com/f/movie/57871f2743ae30c43e91fb893dd4b6f19fc20c0d/js/movie/project/gaia/__init__.js","httpVersion":"HTTP/1.1","cookies":[],"headers":[],"queryString":[],"headersSize":409,"bodySize":0,"comment":""},"response":{"status":200,"statusText":"OK","httpVersion":"HTTP/1.1","cookies":[],"headers":[],"content":{"size":14040,"mimeType":"application/javascript","text":"jQuery.fn.isChildAndSelfOf = function(b){\n        return (this.closest(b).length > 0);\n};\n\n(function(gaiaConfig){\n    var showTimer,\n        LIMITE = 20,\n        queryObj = {},\n        xhr = $.Deferred().resolve(),\n\n        TAG_API = '/j/search_tags',\n        MOVIES_API = '/j/search_subjects',\n        MOVIE_INFO_API = '/j/subject_abstract',\n\n        $GAIA = $('#gaia'),\n        $fliterWp = $GAIA.find('.fliter-wp'),\n        $fliterPlaceholder = $GAIA.find('.fliter-placeholder'),\n        $tagsList = $GAIA.find('.tag-list'),\n        $subTagsList = $GAIA.find('.sub-tag-list'),\n        $list = $GAIA.find('.list'),\n        $detailPop = $GAIA.find('.detail-pop'),\n        $moreBtn = $GAIA.find('.more'),\n\n        tagTmpl = _.template($('#tag-tmpl').html()),\n        subjectTmpl = _.template($('#subject-tmpl').html()),\n        detailTmpl = _.template($('#subject-info-tmpl').html()),\n\n        placeholderHeight = 0;\n\n    $(window).on('scroll', function(){\n        var listHeight = $list.height(),\n            listTop = $list.offset().top,\n            scrollTop = $('body').scrollTop(),\n            top = listTop - scrollTop;\n\n        if(top < 0 && Math.abs(top) < listHeight && gaiaConfig.fixFilter) {\n            if (!$fliterWp.hasClass('fliter-fix')) {\n                $fliterPlaceholder.css('height', placeholderHeight);\n                $fliterWp\n                    .addClass('fliter-fix')\n                    .css({top: '-400px'})\n                    .animate({top: -($('.fliter-fix').outerHeight()-30)}, 300)\n                    .find('.fliter-handle')\n                    .text('当前选择：'+$tagsList.find('.activate').text());\n            }\n        } else {\n            $fliterPlaceholder.css('height', 0);\n            $fliterWp.removeClass('fliter-fix');\n        }\n    });\n\n    $GAIA.on('click', 'h2 a', changeType)\n        .on('click', '.custom-btn', showCustomFrm)\n        .on('click', '.custom-frm button', addCustomTag)\n        .on('click', 'label .remove', removeCustomTag)\n        .on('click', '.more', fetchMore)\n        .on('mouseover', '.cover-wp', showDetail)\n        .on('mouseout',  '.cover-wp', hideDetail)\n        .on('mouseover', '.detail-pop', function(){\n            $(this).show();\n        })\n        .on('mouseout',  '.detail-pop', function(){\n            $(this).hide();\n        })\n        .on('click', '.fliter-fix label input', function(){\n            var pos = $GAIA.offset().top;\n            setTimeout(function(){\n                $('html,body').animate({scrollTop: pos}, 500);\n            }, 10);\n        })\n        .on('mouseover', '.fliter-fix', function(e){\n            $fliterWp.animate({top: 0}, 100);\n        })\n        .on('mouseout', '.fliter-fix', function(e){\n            if(!$(e.relatedTarget).isChildAndSelfOf($fliterWp)) {\n                $fliterWp.animate({top: -($(this).outerHeight()-30)}, 100);\n            }\n        })\n        .on('click','label input', function(e){\n            var $target = $(this);\n            if ($target.attr('name') === 'watched' && !$target.hasClass('me')) {\n                e.preventDefault();\n                $target.removeAttr('checked');\n                $('.login-tip').slideDown('fast');\n                return false;\n            } else {\n                changeTag($target);\n            }\n        });\n\n\n    function changeType(e) {\n        e.preventDefault();\n\n        var $this = $(e.target),\n            type  = $this.data('type');\n\n        //电视需要隐藏掉自定义\n\n        if(type === 'tv') {\n            $GAIA.find('.custom-btn').hide();\n        } else {\n            $GAIA.find('.custom-btn').show();\n        }\n\n        $('#gaia_frm').find('[name=type]').val(type);\n        $this.addClass('activate');\n        $this.siblings('.activate').removeClass('activate');\n\n        fetchTags({type: type});\n    }\n\n    function showCustomFrm(e) {\n        var $target = $(e.target),\n            $frm = $target.siblings('.custom-frm');\n\n        $frm.addClass('inline-block').find('input').focus();\n        $target.hide();\n    }\n\n    function addCustomTag(e) {\n        e.preventDefault();\n\n        var $target = $(e.target),\n            $frm    = $target.parents('.custom-frm'),\n            $newTag = $frm.find('input'),\n            newTag  = $.trim($newTag.val()),\n            type    = $frm.data('type');\n\n        if(newTag) {\n            var tag;\n\n            tagHtml = $(tagTmpl({val: newTag, type: $frm.data('type'), custom: true}));\n\n            if(type === 'tag') {\n                tag = tagHtml.appendTo($tagsList);\n            } else if (type === 'sub_tag') {\n                tag = tagHtml.appendTo($subTagsList);\n            }\n\n            changeTag(tag.find('input'));\n\n            $newTag.val('');\n        }\n\n        $frm.removeClass('inline-block')\n            .siblings('.custom-btn').show();\n    }\n\n    function removeCustomTag(e){\n        e.preventDefault();\n\n        var $tag = $(e.target).parent('label'),\n            firstTag = $tag.parent().find('input:first');\n\n        if ($tag.hasClass('activate')){\n            changeTag(firstTag);\n        }\n        $tag.remove();\n    }\n\n    function changeTag(target){\n        var $tagList = target.parents('.tag-list, .sub-tag-list'),\n            type     = $('#gaia_frm').find('[name=type]').val(),\n            tag_type = target.attr('name');\n\n        if (xhr.state() == 'pending') {\n            xhr.abort();\n        }\n\n        // reset page_start\n        $('[name=page_start]').val(0);\n        $list.empty();\n\n        if ($tagList.length) {\n\n            $tagList.find('.activate').removeClass('activate')\n                    .find('[checked]').removeAttr('checked');\n\n            target.attr('checked', 'checked')\n                  .parent('label')\n                  .addClass('activate');\n        }\n\n\n        if (tag_type === 'tag') {\n            switch (target.val()){\n                case '最新全片': //最新全片需要按时间排序\n                    $GAIA.find('[name=sort][value=rank]').parent().show();\n                    $GAIA.find('[name=sort][value=time]').attr('checked','checked');\n                    break;\n                case '最新': //'最新'需要按时间排序, 并禁用排序\n                    $GAIA.find('[name=sort][value=rank]').parent().show();\n                    $GAIA.find('[name=sort][value=time]').attr('checked','checked');\n                    $GAIA.find('[name=sort]').attr('disabled','disabled');\n                    break;\n                case '豆瓣猜': //'豆瓣猜'隐藏排序\n                    $GAIA.find('[name=sort][value=rank]').parent().hide();\n                    break;\n                default:\n                    $GAIA.find('[name=sort][value=rank]').parent().show();\n                    $GAIA.find('[name=sort]').removeAttr('disabled');\n                    if (!gaiaConfig.hashbang) {\n                        $GAIA.find('[name=sort][value=recommend]').attr('checked','checked');\n                    }\n            }\n\n            fetchTags({type: type, tag: target.val()});\n        } else {\n            fetchMovies();\n        }\n\n        if ($.isFunction(window.render_recommend_groups)) {\n            render_recommend_groups(target.val());\n        }\n\n    }\n\n    function fetchTags(data) {\n        $.get(TAG_API, data).done(function(e){\n\n            //如果请求参数有tag，则需要根据它渲染sub_tag列表\n            if (data.tag){\n                //如果返回有sub_tag,则先渲染它，再fetchMovie\n                if (e.sub_tags) {\n                    renderTags(e.sub_tags, 'sub_tag');\n                } else {\n                    //如果无，就直接fetchMovie\n                    fetchMovies();\n                }\n            } else {\n                //只渲染tag列表\n                renderTags(e.tags, 'tag');\n            }\n        });\n    }\n\n    function renderTags(data, type){\n        var tagHtml, queryTag, querySubTag;\n\n        if(type === 'tag') {\n            $tagsList.empty();\n        } else if (type === 'sub_tag') {\n            $subTagsList.empty();\n            if (data) {\n                $('.sub-tags').show().find('strong').text($('[name=tag][checked]').val());\n            } else {\n                $('.sub-tags').hide();\n            }\n        }\n\n        $.each(data, function(idx, val){\n            tagHtml += tagTmpl({val: val, type: type, custom: false});\n        });\n\n        if(type === 'tag') {\n            $(tagHtml).appendTo($tagsList);\n            if (queryObj.tag) {\n                //这里用来响应url中的tag参数\n                //如果存在就直接选中\n                //如果不存在该tag就临时创建一个并选中\n                queryTag = $tagsList.find('input[value='+queryObj.tag+']');\n                if (queryTag.length) {\n                    changeTag(queryTag);\n                } else {\n                    queryTag = $(tagTmpl({val: queryObj.tag, type: 'tag', custom: false})).appendTo($tagsList);\n                    changeTag($(queryTag).find('input'));\n                }\n            } else {\n                changeTag($tagsList.find('input:first'));\n            }\n        } else if (type === 'sub_tag') {\n            $(tagHtml).appendTo($subTagsList);\n            if (queryObj.sub_tag) {\n                changeTag($subTagsList.find('input[value='+queryObj.sub_tag+']'));\n            } else {\n                changeTag($subTagsList.find('input:first'));\n            }\n        }\n\n        //重新计算筛选区高度, 让placeholder高度更准确\n        placeholderHeight = $fliterWp.height();\n    }\n\n    function fetchMovies(cb) {\n        var data = $('#gaia_frm').serialize();\n\n        xhr = $.get(MOVIES_API, data);\n\n        if (gaiaConfig.hashbang) {\n            //set hashbang\n            window.location.hash = '!' + data;\n        }\n\n        $moreBtn.show().text('载入中...');\n\n        xhr.done(function(e){\n            var subjectCount = e.subjects.length;\n\n            if (subjectCount || $list.find('.item').length) {\n                renderMovies(e.subjects);\n            } else {\n                $list.html('<p>未找到相关影片</p>');\n                $moreBtn.hide();\n            }\n\n\n            if (subjectCount >=  LIMITE) {\n                $moreBtn.show().text('加载更多');\n            } else {\n                $moreBtn.hide();\n            }\n\n            if ($.isFunction(cb)) {\n                cb();\n            }\n        });\n    }\n\n    function renderMovies(movies, empty) {\n        var activated_tag = $(\"[name='tag']:checked\").val(),\n            subjectsHtml;\n\n        if (movies.length) {\n            $.each(movies, function(idx, val){\n                val.tag = activated_tag;\n                subjectsHtml += subjectTmpl(val);\n            });\n\n            $(subjectsHtml).appendTo($list);\n        } else {\n            $list.html('<p>未找到相关影片</p>');\n            $moreBtn.hide();\n        }\n\n    }\n\n    function fetchMore(e) {\n        if (xhr.state() == 'pending') {\n            return;\n        }\n\n        var $target = $(e.currentT